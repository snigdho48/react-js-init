{
  "name": "reactjs-init",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "npx eslint -c eslint.config.js . '**/*.{js,jsx}' --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "prettify": "prettier --write . --config ./.prettierrc.json",
    "push": "yarn run lint && yarn run prettify && git add . && git commit -m \"$(git status | awk '/(use \\\"git restore --staged <file>...\\\" to unstage)/{p=1; next} p')\" && git push"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.0",
    "redux-thunk": "^3.1.0",
    "redux-toolkit": "^1.1.2",
    "tailwind-styled-components": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.5.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.5.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "git-format-staged": "^3.1.1",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "vite": "^5.3.6",
    "vitest": "^1.6.0"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{json,js,ts,jsx,tsx,html}": "prettier --write --ignore-unknown"
  }
}
